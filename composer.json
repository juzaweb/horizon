{
    "name": "juzaweb/horizon",
    "type": "juzaweb-plugin",
    "license": "MIT",
    "description": "",
    "extra": {
        "juzaweb": {
            "name": "Horizon",
            "domain": "horizon",
            "providers": [
                "Juzaweb\\Horizon\\Providers\\HorizonServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\Horizon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juzaweb\\Horizon\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0|^8.1",
        "laravel/horizon": "^5.22"
    },
    "require-dev": {
         "juzaweb/modules": "^1.0",
         "juzaweb/dev-tool": "^1.0"
    }
}
